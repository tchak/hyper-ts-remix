{
  "name": "hyper-ts-remix",
  "version": "0.18.0",
  "author": "Paul Chavard <github@paul.chavard.net>",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^16.7.10",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "release-it": "^14.11.5",
    "rollup": "^2.56.3",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "lint": "npm-run-all lint:*",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "prepare": "tsc && rollup -c rollup.config.js",
    "postinstall": "remix setup"
  },
  "dependencies": {
    "@remix-run/dev": "^0.18.0",
    "@remix-run/node": "^0.18.0",
    "@remix-run/react": "^0.18.0",
    "fp-ts": "^2.11.1",
    "fp-ts-contrib": "^0.1.26",
    "hyper-ts": "^0.7.1",
    "remix": "^0.18.0"
  },
  "prettier": {
    "singleQuote": true
  },
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "require": "./dist/index.cjs"
  },
  "files": [
    "src",
    "dist",
    "Middleware",
    "!dist/**/*.d.ts.map"
  ],
  "engines": {
    "node": ">= 12.*"
  },
  "volta": {
    "node": "14.17.5",
    "yarn": "1.22.10"
  },
  "release-it": {
    "npm": {
      "publish": true,
      "skipChecks": true
    }
  }
}
